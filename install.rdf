     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
  
    <em:id>bibliovie@ens.fr</em:id>
    <em:name>Bibliovie extension</em:name>
    <em:version>3.0</em:version>
    <em:description>Appends bibliovie portal domain .gate1.inist.fr to current URL, which asks for user and password if user is not already logged in, and then (usually) gives access to articles available inside bibliovie service.</em:description>
    <em:creator>Leonardo Barbosa</em:creator>
    <!-- optional items -->
    <em:homepageURL>http://code.google.com/bibliofox</em:homepageURL>
